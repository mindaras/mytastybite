{
  "name": "mytastybite-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:dev": "npm run db:cache:start && wait-on tcp:5432 && wait-on -d 1000 tcp:6379 && npm run db:migrate:dev && npm run db:seed && npm run dev",
    "start:prod": "next start",
    "dev": "next dev -p 80",
    "build": "next build",
    "lint": "next lint",
    "format:schema": "prisma format",
    "db:pull": "prisma db pull",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "ts-node --esm ./prisma/seed.mts",
    "db:studio": "prisma studio",
    "db:cache:start": "npm run db:cache:stop && docker-compose -f dev.docker-compose.yml up --build -d",
    "db:cache:stop": "docker-compose -f dev.docker-compose.yml down -v && docker-compose -f dev.docker-compose.yml rm -f",
    "docker:volumes:list": "docker volume ls",
    "docker:volumes:clear": "docker volume prune -f",
    "sitemap:generate": "ts-node --esm ./prisma/sitemap.mts",
    "rollback": "prisma migrate resolve --rolled-back \"20230929152131_unique_articles_title_without_accents_and_updated_at_field_for_articles\""
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.421.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.43.0",
    "@opentelemetry/resources": "^1.17.0",
    "@opentelemetry/sdk-node": "^0.43.0",
    "@opentelemetry/sdk-trace-node": "^1.17.0",
    "@opentelemetry/semantic-conventions": "^1.17.0",
    "@prisma/client": "^5.3.1",
    "@tiptap/pm": "^2.1.11",
    "@tiptap/react": "^2.1.11",
    "@tiptap/starter-kit": "^2.1.11",
    "@vercel/otel": "^0.3.0",
    "bcrypt": "^5.1.1",
    "classnames": "^2.3.2",
    "client-only": "^0.0.1",
    "eslint": "8.39.0",
    "eslint-config-next": "13.3.4",
    "jimp": "^0.22.10",
    "jsonwebtoken": "^9.0.2",
    "next": "^13.5.1",
    "nodemailer": "^6.9.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "redis": "^4.6.6",
    "remove-accents": "^0.5.0",
    "request-ip": "^3.3.0",
    "server-only": "^0.0.1",
    "typescript": "5.0.4",
    "wait-on": "^7.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^18.16.3",
    "@types/nodemailer": "^6.4.8",
    "@types/react": "18.2.1",
    "@types/react-dom": "18.2.2",
    "@types/request-ip": "^0.0.37",
    "encoding": "^0.1.13",
    "prisma": "^5.3.1",
    "sass": "^1.62.1",
    "ts-node": "^10.9.1"
  }
}
